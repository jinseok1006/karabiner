{
    "title": "Tab Enhancement",
    "rules": [
        {
            "description": "tab+,./ to 123",
            "manipulators": [
                {
                    "from": {
                        "key_code": "tab"
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "tab_pressed",
                                "value": 1
                            }
                        }
                    ],
                    "to_after_key_up": [
                        {
                            "set_variable": {
                                "name": "tab_pressed",
                                "value": 0
                            }
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "tab"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "conditions": [
                        {
                            "name": "tab_pressed",
                            "value": 1,
                            "type": "variable_if"
                        }
                    ],
                    "from": {
                        "key_code": "f13"
                    },
                    "to": [
