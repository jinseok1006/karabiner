    {
      "description": "capslock + hjkl to arrow",
      "manipulators": [
        {
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "caps_lock_pressed",
                "value": 1
              }
            }
